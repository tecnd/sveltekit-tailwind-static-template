    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.4",
    "postcss": "^8.4.14",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5",
